= form_for @setting do |f|
	.form-container
	- if @setting.errors.any?
		#error_explanation
			%h2= "#{pluralize(@setting.errors.count, "error")} prohibited this setting from being saved:"
			%ul
				- @setting.errors.full_messages.each do |message|
					%li= message
	%form
		.row.mb-2
			.col-md-6
				= f.label :image_value
				.custom-file.w-100
					= f.file_field :image_value,:class=> "custom-file-input", :id=>"logo", :required => true
					= f.label :choose_file, :class=> "custom-file-label", :for=>"logo"
			.col-md-6
				= f.label :key
				= f.text_field :key, :class => 'form-control', :required => true
		.row.mb-2
			.col-md-6
				= f.label :text_value
				= f.text_field :text_value, :class => 'form-control', :required => true
			
		.actions
			= f.submit 'Save', :class => 'btn btn-sm btn-primary'
